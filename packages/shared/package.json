{
  "name": "@vjs-ui/shared",
  "version": "0.1.0",
  "description": "VJS-UI 共享类型和常量",
  "keywords": [
    "vjs-ui",
    "shared",
    "types"
  ],
  "homepage": "https://github.com/vjsplus-j/vjs-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vjsplus-j/vjs-ui.git",
    "directory": "packages/shared"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
